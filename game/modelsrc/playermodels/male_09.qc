		flexpair "AU26" 1 frame 16
		flexpair "AU27" 1 frame 17
		flexpair "AU26Z" 1 frame 18
		flexpair "AU27Z" 1 frame 19
		// Already in eyelid lines: flex "upper_right" frame 20
		// Already in eyelid lines: flex "upper_right" frame 21

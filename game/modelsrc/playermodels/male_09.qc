$cdmaterials "models/humans/male/group01"
$cdmaterials "models/humans/male"

$model "male_09" "male_09/male_09_reference.smd" {

	eyeball "eye_right" "ValveBiped.Bip01_Head1" -1.265200 -3.575199 67.870203 "eyeball_r" 1 4 "iris_unused" 0.63
	eyeball "eye_left" "ValveBiped.Bip01_Head1" 1.205800 -3.575199 67.876200 "eyeball_l" 1 -4 "iris_unused" 0.63

	eyelid upper_right "male_09/male_09.vta" lowerer 20 -0.24 neutral 0 0.19 raiser 21 0.28 split 1 eyeball "eye_right"
	eyelid lower_right "male_09/male_09.vta" lowerer 22 -0.3 neutral 0 -0.22 raiser 23 -0.05 split 1 eyeball "eye_right"
	eyelid upper_left "male_09/male_09.vta" lowerer 24 -0.24 neutral 0 0.19 raiser 25 0.28 split -1 eyeball "eye_left"
	eyelid lower_left "male_09/male_09.vta" lowerer 26 -0.3 neutral 0 -0.22 raiser 27 -0.05 split -1 eyeball "eye_left"

	mouth 0 "mouth" "ValveBiped.Bip01_Head1" 0 1 0

	flexfile "male_09/male_09.vta" 
	{
		defaultflex frame 0
		flexpair "AU12" 1 frame 1
		flexpair "AU12" 1 frame 2
		flex "AU12AU25" frame 3
		flexpair "AU15" 1 frame 4
